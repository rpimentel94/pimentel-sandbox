<div{{ attributes.addClass(classes, 'team-members') }}>
    <div class="w-full {{gutter_option}} {{background_style}}" style="background-color:{{background_color}}">
        <div class="w-11/12 xl:w-8/12 m-auto max-w-7xl">
            {% if content.field_display_admin_title.0['#markup'] == "On" %}
                <h2 class="text-4xl font-extralight pb-6">{{ content.field_admin_title }}</h2>
            {% endif %}

            <div class='flex grow flex-col md:flex-row w-full'>
                {{team_members}}
            </div>
        </div>
    </div>
</div>